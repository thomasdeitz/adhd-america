<section id="sponsors">
  <div class="container">
    <div class="row">
    <h1>Sponsors</h1>
    </div>
    <div class="logos row">
      <%- partial("../../_shared/logo") %>
      <%- partial("../../_shared/logo.centare") %>
      <%- partial("../../_shared/awsLogo") %>
      <img src="../../assets/ISCORP-LOGO-REV-ORNG-SML.png">
    </div>
  </div>
</section>

<style>
  #sponsors {
    background-color: #172C5D;
    text-align:center;
    
  }

  #sponsors h1 {
    color:white;
  }
  
  .row.logos {
    padding: 6vmin;
    display:flex;
    justify-content: center;
  }

  .row.logos > * {
    position:relative;
    height: auto;
    max-height:60px;
    width:150px;
    transform: translateY(200%);
    opacity: 0;
    transition: all .5s ease-in-out;
  }
  .row.logos > *:nth-child(2) { transition-delay: .1s;}
  .row.logos > *:nth-child(3) { transition-delay: .2s;}
  .row.logos > *:nth-child(4) { transition-delay: .3s;}


.visible .row.logos > * {
    transform: translateY(0);
    opacity: 1;
}

  .logos svg, .logos svg * {
    fill: white !important;
  }
</style>