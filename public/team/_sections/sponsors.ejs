<section id="sponsors">
  <div class="container">
    <h1>Sponsors</h1>
  </div>
  <div class="container logos">
      <%- partial("../../_shared/logo") %>
      <%- partial("../../_shared/logo.centare") %>
      <%- partial("../../_shared/awsLogo") %>
    </div>
</section>

<style>
  #sponsors {
    background-color: #172C5D;
    text-align:center;
    
  }

  #sponsors h1 {
    color:white;
  }
  
  .container.logos > * {
    position:relative;
    height: auto;
    max-height:60px;
    width:150px;
    transform: translateY(200%);
    opacity: 0;
    transition: all .5s ease-in-out;
    padding: 6vmin 0;
  }
  .container.logos > *:nth-child(2) { transition-delay: .1s;}
  .container.logos > *:nth-child(3) { transition-delay: .2s;}
  .container.logos > *:nth-child(4) { transition-delay: .3s;}


.visible .container.logos > * {
    transform: translateY(0);
    opacity: 1;
}

  .logos svg, .logos svg * {
    fill: white !important;
  }
</style>